<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.hna.hka.archive.management.business.dao.FilingCycleMapper">

    <select id="getCycle" resultType="com.hna.hka.archive.management.business.model.BusinessFilingCycle">
        select ID , FILING_CYCLE as filingCycle from BUSIMESS_FILING_CYCLE
    </select>


    <update id="editFilingCycle">
        update BUSIMESS_FILING_CYCLE = #{cycle} where id = 1
    </update>



</mapper>