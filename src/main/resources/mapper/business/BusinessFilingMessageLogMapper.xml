<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.hna.hka.archive.management.business.dao.BusinessFilingMessageLogMapper" >


    <select id="getFilingMessageLogList" resultType="com.hna.hka.archive.management.business.model.BusinessFilingMessageLog">
        select *
        from BUSINESS_FILING_MESSAGE_LOG
        <where>
            <if test="messageId != null and messageId != ''">
                MESSAGE_ID = #{messageId}
            </if>
        </where>
        order by CREATE_TIME desc
    </select>


</mapper>