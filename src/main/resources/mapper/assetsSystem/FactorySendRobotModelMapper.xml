<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.hna.hka.archive.management.assetsSystem.dao.FactorySendRobotModelMapper">

    <select id="list" resultType="com.hna.hka.archive.management.assetsSystem.model.FactorySendRobotModel">
        select * from SYS_SCENIC_FACTORY_SEND_MODEL
        where ROBOT_FACTORY_SEND_ROBOT_ID = #{id}
    </select>

    <insert id="add" parameterType="com.hna.hka.archive.management.assetsSystem.model.FactorySendRobotModel">
        insert into SYS_SCENIC_FACTORY_SEND_MODEL(ID, APPLICABLE_MODEL,ROBOT_COUNT,CREATE_DATE,ROBOT_FACTORY_SEND_ROBOT_ID)
        VALUES (#{id}, #{applicableModel}, #{robotCount},#{createDate},#{robotFactorySendRobotId})
    </insert>

    <update id="edit" parameterType="com.hna.hka.archive.management.assetsSystem.model.FactorySendRobotModel">
        update SYS_SCENIC_FACTORY_SEND_MODEL
        set APPLICABLE_MODEL   = #{applicableModel},
        ROBOT_COUNT    = #{robotCount},
        CREATE_DATE  =  #{createDate},
        ROBOT_FACTORY_SEND_ROBOT_ID = #{robotFactorySendRobotId}
        where ID = #{id}
    </update>

    <delete id="delete">
        delete
        from SYS_SCENIC_FACTORY_SEND_MODEL
        where ID = #{id}
    </delete>

    <delete id="deleteFactorySendRobotModelId">
        delete
        from SYS_SCENIC_FACTORY_SEND_MODEL
        where ROBOT_FACTORY_SEND_ROBOT_ID = #{id}
    </delete>
</mapper>